landrs:AgentRole  rdf:type rdfs:Class, owl:Class;
                  rdfs:label "AgentRole" .

###  https://schema.landrs.org/schema/Agent
landrs:Agent  rdf:type owl:Class ;
              rdfs:label "Agent" .

###  https://schema.landrs.org/schema/Researcher
landrs:Researcher rdf:type owl:Class ;
                  rdfs:subClassOf landrs:AgentRole ;
                  rdfs:label "Researcher" .

###  https://schema.landrs.org/schema/UxVOperator
landrs:UxVOperator  rdf:type owl:Class, prov:SoftwareAgent ;
                    rdfs:subClassOf landrs:AgentRole ;
                    rdfs:label "UxVOperator" .


###  http://schema.landrs.org/schema/providesAgentRole
landrs:providesAgentRole  rdf:type owl:ObjectProperty ;
                                rdfs:domains landrs:Flight ;
                                rdfs:range landrs:AgentRole ;
                                rdfs:label "providesAgentRole" .

###  http://schema.landrs.org/schema/Place
landrs:Place  rdf:type rdfs:Class, owl:Class ;
              rdfs:subClassOf schema:Place ;
              rdfs:label "Place"@en .

###  https://schema.org/location
schema:location rdf:type owl:ObjectProperty ;
                rdfs:domain landrs:Event ;
                rdfs:range landrs:Place ;
                rdfs:label "location" .

