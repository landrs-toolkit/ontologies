# https://docs.ogc.org/per/20-020.html#_equipment_ontology
# Vocabulary definnitionns using RDFS (schema.org) class properties
# with labels and subClassOf relationships
# https://spinrdf.org/shacl-and-owl.html
@prefix : <https://w3id.org/drone/equipment#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix equipment: <https://w3id.org/drone/equipment#> .
@prefix agents: <https://w3id.org/drone/agents#> .
@prefix identifier: <https://w3id.org/drone/identifier#> .
@base <https://w3id.org/drone/equipment> .

<https://w3id.org/drone/equipment> rdf:type owl:Ontology, voaf:Vocabulary, prov:Entity ;
                            rdfs:label "equipment" ;
                            owl:versionIRI <https://raw.githubusercontent.com/landrs-toolkit/ontologies/master/equipment.ttl> ;
                            dct:contributor [ schema:affiliation [ rdf:type schema:Organization ;
                                                                   foaf:name "University of Notre Dame, Center for Research Computing"@en ;
                                                                   schema:url <https://crc.nd.edu>
                                                                 ] ;
                                              foaf:name "Priscila Moreira"@en
                                            ] ,
                                            [ schema:affiliation [ rdf:type schema:Organization ;
                                                                   foaf:name "University of Notre Dame, Center for Research Computing"@en ;
                                                                   schema:url <https://crc.nd.edu>
                                                                 ] ;
                                              foaf:name "Chris Sweet"@en
                                            ] ,
                                            [ schema:affiliation [ rdf:type schema:Organization ;
                                                                   foaf:name "University of Cape Town"@en ;
                                                                   schema:url <https://www.uct.ac.za>
                                                                 ] ;
                                             foaf:name "Jane Wyngaard"@en
                                            ] ;
                            dct:creator [ schema:affiliation [ a schema:Organization ;
                                                               foaf:name "University of Notre Dame, Center for Research Computing"@en ;
                                                               schema:url <https://crc.nd.edu>
                                                             ] ;
                                          schema:identifier <https://orcid.org/0000-0003-4091-6059> ;
                                          rdfs:seeAlso <https://orcid.org/0000-0003-4091-6059> ;
                                          foaf:name "Charles F Vardeman II"@en
                                        ] ;
                            schema:funder [ rdf:type schema:Organization ;
                                            foaf:name "Alfred P. Sloan Foundation"@en ;
                                            schema:url "https://sloan.org/"
                                          ] ;
                            vann:preferredNamespacePrefix "equipment" ;
                            vann:preferredNamespaceUri "http://w3id.org/drone/equipment" ;
                            dct:description "LANDRS ontology for UxV data interoperability."@en ;
                            dct:rights "This vocabulary is distributed under Creative Commons Attribution 4.0 License - http://creativecommons.org/licenses/by/4.0"@en ;
                            dct:license <https://spdx.org/licenses/CC-BY-4.0> ;
                            schema:license "https://spdx.org/licenses/CC-BY-4.0"@en ;
                            dct:title "LANDRS Equipment"@en ;
                            rdfs:comment "LANDRS ontology for UxV data interoperability."@en ;
                            owl:versionInfo "1.0" ;
                            foaf:maker [ foaf:homepage <https://www.landrs.org> ;
                                         foaf:name "Linked-data API for Networked Drones (LANDRS) project"@en
                                       ] ;
                           # owl:imports <https://w3id.org/drone/agents> ;
                           #  owl:imports <https://w3id.org/drone/identifier> ;
                            .



#################################################################
#    Annotation properties
#################################################################

#################################################################
#    Object Properties
#################################################################
equipment:ownerOf a rdf:Property ;
  rdfs:subPropertyOf schema:owns ;
  rdfs:label "ownerOf"@en ;
  rdfs:comment "Agent role of ownership of some equipment"@en ;
  schema:domainIncludes agents:Agent;
  schema:rangeIncludes equipment:Equipment ;
.

equipment:ownedBy a rdf:Property ;
  rdfs:label "ownedBy"@en ;
  owl:inverseOf equipment:ownerOf ;
  schema:domainIncludes equipment:Equipment ;
  schema:rangeIncludes agents:Agent ;
.

equipment:operatedBy a rdf:Property ;
  rdfs:label "operatedBy"@en ;
  schema:domainIncludes equipment:Equipment ;
  schema:rangeIncludes  agents:Agent ;
.

equipment:registeredBy a rdf:Property ;
  rdfs:label "registeredBy" ;
  schema:domainIncludes equipment:Equipment ;
  schema:rangeIncludes  agents:agent ;
.




equipment:equipmentType a rdf:Property ;
  rdfs:label "equipmentType"@en ;
  rdfs:comment "A type class of equipment"@en ;
.

equipment:equipmentModel a rdf:Property ;
  rdfs:label "equipmentModel"@en ;
  rdfs:subPropertyOf schema:model ;
  schema:domainIncludes schema:Product ;
    schema:rangeIncludes equipment:EquipmentModel,
        schema:Text ;
.

equipment:manufacturedBy a rdf:Property ;
  rdfs:subPropertyOf schema:manufacturer ;
  rdfs:label "manufacturedBy"@en ;
.



#################################################################
#    Data properties
#################################################################
equipment:modelNumber a owl:DataTypeProperty ;
  rdfs:label "model number" ;
  schema:domainIncludes equipment:Equipment ;
  schema:rangeIncludes xsd:string ;
.

equipment:registrationNumber a owl:DataTypeProperty ;
  rdfs:label "registration number" ;
  schema:domainIncludes equipment:Equipment ;
  schema:rangeIncludes xsd:string ;
.

equipment:registrationDate a owl:DataTypeProperty ;
  rdfs:label "registration date" ;
  schema:domainIncludes equipment:Equipment ;
  schema:rangeIncludes xsd:dateTime ;
.

identifier:identifier a owl:DataTypeProperty ;
  schema:domainIncludes equipment:Equipment ;
  schema:rangeIncludes xsd:string ;
.
#################################################################
#    Classes
#################################################################
equipment:Equipment a rdfs:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf schema:Product ;
  rdfs:label "Equipment"@en ;
.

equipment:Manufacturer a rdfs:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf prov:Agent ;
  rdfs:subClassOf schema:Organization ;
  rdfs:label "Manufacturer"@en ;
.

equipment:EquipmentModel a rdfs:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf schema:ProductModel ;
  rdfs:label "Equipment Model"@en ;
.

equipment:EquipmentType a rdfs:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:label "Equipment Type"@en ;
.

equipment:EquipmentOwner a rdfs:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf agents:agentRole ;
  rdfs:label "Equipment Owner"@en
.

equipment:EquipmentPart a rdfs:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:label "Equipment Part"@en ;
.

equipment:EquipmentOperator a rdfs:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf agents:AgentRole ;
  rdfs:label "Equipment Operator"@en ;
.

equipment:Identifier a rdfs:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:label "Identifier"@en ;
.
equipment:Product a rdfs:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf schema:Product ;
  rdfs:label "Product"@en ;
.

