###  http://purl.org/dc/elements/1.1/description
dct:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dct:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/contributor
dct:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
dct:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dct:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dct:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
dct:issued rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dct:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
dct:publisher rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/source
dct:source rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .

### https://schema.org/domainInculdes
schema:domainIncludes rdf:type owl:AnnotationProperty .

### https://schema.org/rangeIncludes
schema:rangeIncludes rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################
equipment:ownerOf a owl:ObjectProperty ;
  rdfs:subPropertyOf schema:owns ;
  rdfs:label "ownerOf"@en ;
  rdfs:comment "Agent role of ownership of some equipment"@en ;
  schema:domainIncludes schema:;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (agents:AgentRole
                  prov:Agent
                  equipment:EquipmentOwner
    )
  ] ;
  rdfs:range [
    a owl:Class ;
    owl:unionOf(
    schema:Product
    equipment:Equipment
    )
  ]
.

equipment:ownedBy a owl:ObjectProperty ;
  rdfs:label "ownedBy"@en ;
  owl:inverseOf equipment:ownerOf ;
  rdfs:domain equipment:Equipment ;
  rdfs:range agents:Agent ;
.

equipment:operatedBy a owl:ObjectProperty ;
  rdfs:label "operatedBy"@en ;
  rdfs:domain equipment:Equipment ;
  rdfs:range  agents:Agent ;
.

equipment:registeredBy a owl:ObjectProperty ;
  rdfs:label "registeredBy" ;
  rdfs:domain equipment:Equipment ;
  rdfs:range  agents:agent ;
.




equipment:equipmentType a owl:ObjectProperty ;
  rdfs:label "equipmentType"@en ;
  rdfs:comment "A type class of equipment"@en ;
.

equipment:equipmentModel a owl:ObjectProperty ;
  rdfs:label "equipmentModel"@en ;
  rdfs:subPropertyOf schema:model ;
  schema:domainIncludes schema:Product ;
    schema:rangeIncludes equipment:EquipmentModel,
        schema:Text ;
.

equipment:manufacturedBy a owl:ObjectProperty ;
  rdfs:subPropertyOf schema:manufacturer ;
  rdfs:label "manufacturedBy"@en ;
.


#################################################################
#    Data properties
#################################################################
equipment:modelNumber a owl:DataTypeProperty ;
  rdfs:label "model number" ;
  rdfs:domain equipment:Equipment ;
  rdfs:range xsd:string ;
.

equipment:registrationNumber a owl:DataTypeProperty ;
  rdfs:label "registration number" ;
  rdfs:domain equipment:Equipment ;
  rdfs:range xsd:string ;
.

equipment:registrationDate a owl:DataTypeProperty ;
  rdfs:label "registration date" ;
  rdfs:domain equipment:Equipment ;
  rdfs:range xsd:dateTime ;
.

identifier:identifier a owl:DataTypeProperty ;
  rdfs:domain equipment:Equipment ;
  rdfs:range xsd:string ;
.
#################################################################
#    Classes
#################################################################
equipment:Equipment a owl:Class ;
  rdfs:subClassOf schema:Product ;
  rdfs:label "Equipment"@en ;
.

equipment:Manufacturer a owl:Class ;
  rdfs:subClassOf prov:Agent ;
  rdfs:subClassOf schema:Organization ;
  rdfs:label "Manufacturer"@en ;
.

equipment:EquipmentModel a owl:Class ;
  rdfs:subClassOf schema:ProductModel ;
  rdfs:label "Equipment Model"@en ;
.

equipment:EquipmentType a owl:Class ;
  rdfs:label "Equipment Type"@en ;
.

equipment:EquipmentOwner a owl:Class ;
  rdfs:subClassOf agents:agentRole ;
  rdfs:label "Equipment Owner"@en
.

equipment:EquipmentPart a owl:Class ;
  rdfs:label "Equipment Part"@en ;
.

equipment:EquipmentOperator a owl:Class ;
  rdfs:subClassOf agents:AgentRole ;
  rdfs:label "Equipment Operator"@en ;
.

equipment:Identifier a owl:Class ;
  rdfs:label "Identifier" ;
.

