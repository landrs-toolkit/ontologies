# baseURI: https://w3id.org/drone/agents
# imports: http://www.w3.org/2006/time
# imports: http://www.w3.org/ns/prov

@prefix : <https://w3id.org/drone/agents#> .
@prefix agents: <https://w3id.org/drone/agents#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

agents:Agent
  rdf:type rdfs:Class ;
  rdfs:label "Agent" ;
.

agents:AgentRole
  rdf:type rdfs:Class ;
  rdfs:label "Agent role" ;
.
agents:RoleDuration
  rdf:type rdfs:Class ;
  rdfs:label "Role Duration" ;
  rdfs:subClassOf <http://www.w3.org/2006/time#Interval> ;
.

agents:interval
  rdf:type rdf:Property ;
  rdfs:label "interval" ;
  schema:domainIncludes agents:AgentRole ;
  schema:rangeIncludes agents:RoleDuration ;
.

agents:isAgentRoleIn
  rdf:type rdf:Property ;
  rdfs:label "is agent role in" ;
.
agents:isPerformedBy
  rdf:type rdf:Property ;
  rdfs:label "is performed by" ;
  schema:domainIncludes agents:AgentRole ;
  schema:inverseOf agents:performsAgentRole ;
  schema:rangeInclude agents:Agent ;
.
agents:performsAgentRole
  rdf:type rdf:Property ;
  rdfs:label "performs agent role" ;
  schema:domainIncludes agents:Agent ;
  schema:inverseOf agents:isPerformedBy ;
  schema:rangeIncludes agents:AgentRole ;
.
agents:providesAgentRole
  rdf:type rdf:Property ;
  rdfs:label "provides agent role" ;
.
<https://w3id.org/drone/agents>
  rdf:type owl:Ontology ;
  rdf:type prov:Entity ;
  owl:imports <http://topbraid.org/schema/> ;
  owl:imports <http://www.w3.org/2006/time> ;
  owl:imports <http://www.w3.org/ns/prov> ;
.
