#################################################################
#    Object Properties
#################################################################
equipment:ownerOf a owl:ObjectProperty ;
  rdfs:label "ownerOf"@en ;
  rdfs:comment "Agent role of ownership of some equipment"@en ;
  schema:domainIncludes
  rdfs:domain [ a owl:Class ;
    owl:unionOf ( agents:AgentRole
                  prov:Agent
                  equipment:EquipmentOwner
    )
  ] ;
  rdfs:range equipment:Equipment ;
.

equipment:equipmentType a owl:ObjectProperty ;
  rdfs:label "equipmentType"@en ;
  rdfs:comment ""@en ;
.

equipment:equipmentModel a owl:ObjectProperty ;
  rdfs:label "equipmentModel"
  rdfs:subClassOf schema:model ;
  schema:domainIncludes schema:Product ;
    schema:rangeIncludes landrs:EquipmentModel,
        schema:Text ;
.


#################################################################
#    Data properties
#################################################################


#################################################################
#    Classes
#################################################################
equipment:Equipment a owl:Class ;
  rdfs:subClassOf schema:Product ;
  rdfs:label "Equipment"@en ;
.

equipment:Manufacturer a owl:Class ;
  rdfs:subClassOf prov:Agent ;
  rdfs:subClassOf schema:Organization ;
  rdfs:label "Manufacturer"@en ;
.

equipment:EquipmentModel a owl:Class ;
  rdfs:subClassOf schema:ProductModel ;
  rdfs:label "Equipment Model"@en ;
.

equipment:EquipmentType a rdfs:Class ;
  rdfs:label "Equipment Type"@en ;
.

equipment:EquipmentOwner a rdfs:Class ;
  rdfs:label "Equipment Owner"@en
.

equipment:EquipmentPart a rdfs:Class ;
  rdfs:label "Equipment Part"@en ;
.

equipment:EquipmentOperator a rdfs:Class ;
  rdfs:subClassOf prov:Agent ;
  rdfs:label "Equipment Operator"@en ;
.

